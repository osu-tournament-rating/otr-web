{
  "name": "otr",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "lint": "bun run --filter web lint && bun run --filter data-worker lint",
    "build": "bun run --filter web build && bun run --filter data-worker build",
    "start": "bun run --filter web start && bun run --filter data-worker start",
    "dev": "sh -c 'trap \"kill -TERM 0\" INT TERM EXIT; bun run --filter web dev & bun run --filter data-worker dev' & wait",
    "format": "bunx prettier . --write"
  },
